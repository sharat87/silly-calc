{
	"name": "silly-calc",
	"version": "1.0.0",
	"description": "A silly notepad-style calculator.",
	"repository": "https://github.com/sharat87/silly-calc.git",
	"author": "Shrikant Sharat Kandula",
	"license": "MIT",
	"scripts": {
		"start": "parcel serve --port 3050 --no-autoinstall index.html",
